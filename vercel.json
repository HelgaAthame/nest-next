{
  "version": 2,
  "github": false,
  "builds": [
    { "src": "back/src/main.ts", "use": "@vercel/node" },
    { "src": "front/next.config.mjs", "use": "@vercel/next" }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "back/src/main.ts",
      "methods": ["GET", "POST", "PUT", "DELETE"]
    }
  ]
}
